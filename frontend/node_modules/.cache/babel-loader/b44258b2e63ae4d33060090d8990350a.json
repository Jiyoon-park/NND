UAAA,EAAA;AAAA,iBAAA,uBAAA;AAAA;AAHA;AAVA,KAAA;AAAA,GALA;AAsBA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAJA;AAKA,IAAA,KALA,mBAKA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,KAPA;AAQA,IAAA,eARA,6BAQA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA,KAVA;AAWA,IAAA,aAXA,2BAWA;AAAA;;AACA,MAAA,KAAA,CACA,GADA,qCAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,KAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AAAA,OANA;AAOA;AAnBA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <v-row justify=\"center\">\r\n    <NavBar />\r\n    <v-col cols=\"10\" md=\"8\" lg=\"6\" class=\"mt-15\">\r\n      <h2>로그인</h2>\r\n      <v-form class=\"form\" ref=\"form\" v-model=\"valid\" lazy-validation>\r\n        <v-text-field\r\n          v-model=\"email\"\r\n          :rules=\"emailRules\"\r\n          label=\"이메일\"\r\n          outlined\r\n          dense\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\r\n          :rules=\"[rules.required, rules.min]\"\r\n          :type=\"show ? 'text' : 'password'\"\r\n          name=\"input-10-2\"\r\n          label=\"비밀번호\"\r\n          @click:append=\"show = !show\"\r\n          outlined\r\n          dense\r\n        ></v-text-field>\r\n        <v-btn class=\"button\" :disabled=\"!valid\" color=\"teal\" @click=\"validate\"\r\n          >로그인</v-btn\r\n        >\r\n        <v-checkbox\r\n          v-model=\"checkbox\"\r\n          color=\"success\"\r\n          label=\"로그인 정보 기억\"\r\n          required\r\n        ></v-checkbox>\r\n      </v-form>\r\n      <div class=\"login-body text-center\">\r\n        <div class=\"sns-login\">\r\n          <p>SNS로 간편하게 로그인할 수 있어요!</p>\r\n          <v-btn\r\n            color=\"yellow darken-1\"\r\n            class=\"sns-btn\"\r\n            @click=\"snsLoginKakao\"\r\n            dark\r\n            >Kakao</v-btn\r\n          >\r\n          <v-btn color=\"green accent-4\" class=\"sns-btn\" dark>Naver</v-btn>\r\n          <v-btn color=\"light-blue darken-2\" class=\"sns-btn\" dark>Google</v-btn>\r\n        </div>\r\n        <div class=\"add-option\">\r\n          <router-link to=\"/signup\" class=\"routers\">회원가입</router-link>|\r\n          <router-link to=\"/findemail\" class=\"routers\">이메일 찾기</router-link\r\n          >|\r\n          <router-link to=\"/findpw\" class=\"routers\">비밀번호 찾기</router-link>\r\n        </div>\r\n      </div>\r\n    </v-col>\r\n  </v-row>\r\n</template>\r\n<script>\r\nimport NavBar from \"../components/common/NavBar.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  components: {\r\n    NavBar,\r\n  },\r\n  data: () => ({\r\n    valid: true,\r\n    email: \"\",\r\n    emailRules: [\r\n      (v) => !!v || \"이메일을 입력해주세요\",\r\n      (v) => /.+@.+\\..+/.test(v) || \"올바른 양식의 이메일을 입력해주세요\",\r\n    ],\r\n    checkbox: false,\r\n    show: false,\r\n    password: \"Password\",\r\n    rules: {\r\n      required: (value) => !!value || \"비밀번호를 입력해주세요.\",\r\n      min: (v) => (v && v.length >= 8) || \"비밀번호는 8글자 이상 입력해주세요\",\r\n      emailMatch: () => \"이메일과 비밀번호가 일치하지 않습니다.\",\r\n    },\r\n  }),\r\n\r\n  methods: {\r\n    validate() {\r\n      this.$refs.form.validate();\r\n      this.$router.push(\"/\");\r\n    },\r\n    reset() {\r\n      this.$refs.form.reset();\r\n    },\r\n    resetValidation() {\r\n      this.$refs.form.resetValidation();\r\n    },\r\n    snsLoginKakao() {\r\n      axios\r\n        .get(`http://localhost:8080/loginKakao`)\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.$router.push(\"/\");\r\n        })\r\n        .catch((error) => console.log(error.response.data));\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.login-body {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  /* min-height: 100vh; */\r\n}\r\n\r\n.container {\r\n  width: 300px;\r\n}\r\nh2 {\r\n  text-align: center;\r\n  margin: 0 0 20px;\r\n}\r\n\r\n.form button {\r\n  cursor: pointer;\r\n  color: #fff;\r\n  display: block;\r\n  font-size: 16px;\r\n  width: 100%;\r\n}\r\n\r\n.sns-login {\r\n  margin: 20px 0;\r\n}\r\n\r\n.sns-login .sns-btn {\r\n  margin: 0 4px;\r\n  padding: 5px 15px;\r\n}\r\n\r\n.add-option .routers {\r\n  margin: 0 5px;\r\n  text-decoration: none;\r\n  color: #222;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}